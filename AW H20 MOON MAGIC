                local pos = Hum.CFrame
                local pos2 = pos.p
                local x = math.round(pos2.X)
                local y = math.round(pos2.Y)
                local z = math.round(pos2.Z)
                local args = {
                    [1] = "IcePath",
                    [2] = "Create",
                    [3] = CFrame.new(x,y,z)
                }
                
                game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))            
            end
        end
    end
end)
M:AddButton("Boil Everyone",function()
