                    [2] = "BoilStartTerrain",
                    [3] =  Vector3.new(x,y,z),
                    [4] = Target
                }
                game:GetService("ReplicatedStorage").Events.PowerFunction:InvokeServer(unpack(args))       
            end
        end
    end
end)
M:AddButton("Clean Up Area",function()
    for i=1,100 do
        local Pyrokinesis = game:GetService("Workspace"):FindFirstChild("Pyrokinesis")
        local IcePath =  game:GetService("Workspace"):FindFirstChild("IcePath")
        local BoilPart = game:GetService("Workspace"):FindFirstChild("BoilPart")
        local WaterBall = game:GetService("Workspace"):FindFirstChild("WaterBall")
        if Pyrokinesis then
            Pyrokinesis:Destroy()
        end
        if IcePath then
            IcePath:Destroy()
        end
        if BoilPart then
            BoilPart:Destroy()
        end
        if WaterBall then
            WaterBall:Destroy()
        end
    end
end)
