M:AddButton("Turn Everyone To Mermaid Form",function()
    local plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do
        if Player.Character ~= nil and Player ~= plr then
            local mag = (Player.Character.UpperTorso.Position - plr.Character.UpperTorso.Position).magnitude
            if mag < 999999 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                local args = {
                    [1] = "Hydrokinesis",
                    [2] = "CreateWaterBall",
                    [3] = Target.HumanoidRootPart.CFrame.p,
                    [4] = false
                }
                
                game:GetService("ReplicatedStorage").Events.PowerFunction:InvokeServer(unpack(args))                
            end
        end
    end
    wait(5)
    for i=1,100 do
        local args = {
            [1] = "Hydrokinesis",
            [2] = "TrackPlr",
            [3] = workspace.WaterBall
        }
        
        game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
    wait(0.01)    
        local args = {
            [1] = "Hydrokinesis",
            [2] = "DropBall",
            [3] = workspace.WaterBall
        }
        
        game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
    end
end)
M:AddButton("Ice Path Everyone",function()
    local plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do
        if Player.Character ~= nil and Player ~= plr then
            local mag = (Player.Character.UpperTorso.Position - plr.Character.UpperTorso.Position).magnitude
            if mag < 999999 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
