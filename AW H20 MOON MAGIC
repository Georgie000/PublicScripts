M1:AddButton("Remove All Things",function()
    local Players = game:GetService("Players")
    local alla = Players:GetChildren()
    for i,v in pairs(alla) do
        local everyone = v.Character
        local Players = game:GetService("Players")
        local Workz = game:GetService("Workspace")
        local all = Workz:GetChildren()
        for i,v in pairs(all) do
            if v == everyone then
            else
                local args = {
                [1] = "HydroThermo",
                [2] = "TrackThermo",
                [3] = v
            }
            
            game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
            local args = {
                [1] = "HydroThermo",
                [2] = "BoilEndTerrain",
                [3] = v
            }
            
            game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
            end
        end
    end
end)
local a = M1:AddButton("Remove All Things From Replicated Storage",function()
    local Targeting = game:GetService("ReplicatedStorage")
    local all = Targeting:GetChildren()
    for i,v in pairs(all) do
        if v == game:GetService("ReplicatedStorage").Events.PowerEvent then

        else
            local args = {
                [1] = "HydroThermo",
                [2] = "TrackThermo",
                [3] = v
            }
            
            game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
            local args = {
                [1] = "HydroThermo",
                [2] = "BoilEndTerrain",
                [3] = v
            }
            
            game:GetService("ReplicatedStorage").Events.PowerEvent:FireServer(unpack(args))
        end
    end
end)
a:AddToolTip("You Cant Chat/Spawn if you do this")
